public class iloczynWektorow {

    public static int iloczyn(int[] tab1, int[] tab2) {
        int sum = 0;

        for (int i = 0; i < tab1.length; i++) {
            sum = sum+ (tab1[i] * tab2[i]);

        }

    return sum;

    }

    public static void main(String[] args) {

        int tab1[] = {5, 6},
            tab2[] = {2, 4};

        System.out.println(iloczyn(tab1, tab2));

    }
}
