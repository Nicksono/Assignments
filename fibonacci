public class fibonacci {
    
        public static int fib(int i) {
                   
            if (i == 0 || i == 1) {
                return i;
        }
            else
                return fib(i-1) + fib(i-2);
        }
        
    public static void main(String[] args) {
        
        int x = 30;
        
        System.out.println(fib(x));
    }
    
}
